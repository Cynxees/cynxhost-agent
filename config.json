{
  "app": {
    "privateIp": "${APP_PRIVATE_IP}",
    "publicIp": "${APP_PUBLIC_IP}",
    "address": "0.0.0.0",
    "port": 3001,
    "websocketPort": 8000,
    "name": "CYNXHOST-AGENT",
    "debug": true
  },
  "central": {
    "privateIp": "${CENTRAL_PRIVATE_IP}",
    "publicIp": "${CENTRAL_PUBLIC_IP}",
    "port": "${CENTRAL_PORT}"
  },
  "docker": {
    "containerName": "cynxhost-container",
    "host": "cynx.buzz",
    "sshPort": 22,
    "username": "cynxhost",
    "password": "cynxh0stpass",
    "tmux": {
      "sessionName": "cynxhost-node"
    },
    "files": {
      "volumePath": "/home/cynxhost/cynxhost-agent",
      "minecraftLogFilePath": "/home/cynxhost/cynxhost-agent/output.log",
      "minecraftServerProperties": "/home/cynxhost/node/server.properties"
    }
  },
  "router": {
    "default": "/cynxapi/v1/"
  },
  "ecr": {
    "region": "ap-southeast-1",
    "username": "AWS",
    "password": "",
    "registry": "242201306378.dkr.ecr.ap-southeast-1.amazonaws.com"
  },
  "database": {
    "mysql": {
      "host": "${CENTRAL_PUBLIC_IP}",
      "port": 3306,
      "database": "cynxhost",
      "username": "cynx",
      "password": "pass",
      "dialect": "mysql",
      "pool": {
        "max": 5,
        "min": 0,
        "acquire": 30000,
        "idle": 10000
      }
    }
  },
  "logging": {
    "level": "info",
    "format": "json",
    "destinations": ["console", "elasticsearch"]
  },
  "security": {
    "jwt": {
      "secret": "${JWT_SECRET}",
      "expiresInHour": 72
    },
    "cors": {
      "enabled": true,
      "origins": ["http://localhost:3000", "https://cynxhost-web.vercel.app", "https://localhost:3000", "https://web.cynx.buzz:3000", "https://app.cynx.buzz"],
      "domain": ""
    }
  }
}
