{
  "app": {
    "privateIp": "${APP_PRIVATE_IP}",
    "publicIp": "${APP_PUBLIC_IP}",
    "address": "0.0.0.0",
    "port": 3001,
    "websocketPort": 8000,
    "name": "CYNXHOST-AGENT",
    "debug": false
  },
  "central": {
    "privateIp": "${CENTRAL_PRIVATE_IP}",
    "publicIp": "${CENTRAL_PUBLIC_IP}",
    "port": "${CENTRAL_PORT}"
  },
  "docker": {
    "containerName": "cynxhost-container",
    "host": "cynx.buzz",
    "sshPort": 22,
    "username": "cynxhost",
    "password": "cynxh0stpass",
    "tmux": {
      "sessionName": "cynxhost-node"
    },
    "files": {
      "minecraftLogFilePath": "/home/cynxhost/cynxhost-agent/output.log",
      "minecraftServerProperties": "/home/cynxhost/node/server.properties"
    }
  },
  "router": {
    "default": "/api/v1/"
  },
  "database": {
    "mysql": {
      "host": "${CENTRAL_PUBLIC_IP}",
      "port": 3306,
      "database": "cynxhost",
      "username": "cynx",
      "password": "pass",
      "dialect": "mysql",
      "pool": {
        "max": 5,
        "min": 0,
        "acquire": 30000,
        "idle": 10000
      }
    }
  },
  "logging": {
    "level": "info",
    "format": "json",
    "destinations": ["console", "elasticsearch"]
  },
  "security": {
    "jwt": {
      "secret": "${JWT_SECRET}",
      "expiresInHour": 72
    },
    "cors": {
      "enabled": true,
      "origin": "*"
    }
  }
}
