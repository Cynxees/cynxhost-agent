{
  "app": {
    "privateIp": "${APP_PRIVATE_IP}",
    "publicIp": "${APP_PUBLIC_IP}",
    "address": "0.0.0.0",
    "port": 3001,
    "websocketPort": 8000,
    "name": "CYNXHOST-AGENT",
    "debug": false,
    "persistentNodeId": "${PERSISTENT_NODE_ID}"
  },
  "central": {
    "privateIp": "${CENTRAL_PRIVATE_IP}",
    "publicIp": "${CENTRAL_PUBLIC_IP}",
    "port": "${CENTRAL_PORT}"
  },
  "log": {
    "minecraftLogFilePath": "/home/cynxhost/cynxhost-agent/output.log"
  },
  "router": {
    "default": "/api/v1/"
  },
  "database": {
    "mysql": {
      "host": "47.129.250.117",
      "port": 3306,
      "database": "cynxhost",
      "username": "cynx",
      "password": "pass",
      "dialect": "mysql",
      "pool": {
        "max": 5,
        "min": 0,
        "acquire": 30000,
        "idle": 10000
      }
    }
  },
  "logging": {
    "level": "info",
    "format": "json",
    "destinations": ["console", "elasticsearch"]
  },
  "security": {
    "jwt": {
      "secret": "${JWT_SECRET}",
      "expiresInHour": 72
    },
    "cors": {
      "enabled": true,
      "origin": "*"
    }
  }
}
